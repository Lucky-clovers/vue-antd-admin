hot-search\">\r\n    <a-row style=\"margin: 0 -34px\">\r\n      <a-col style=\"padding: 0 34px; margin-bottom: 24px\" :sm=\"12\" :xs=\"24\">\r\n        <div class=\"num-info\">\r\n          <span class=\"title\">\r\n            {{$t('search')}}\r\n            <a-tooltip :title=\"$t('introduce')\">\r\n              <a-icon type=\"info-circle\" style=\"font-size: 14px; margin-left: 8px\" />\r\n            </a-tooltip>\r\n          </span>\r\n          <div class=\"value\">\r\n            <span class=\"total\">12321</span>\r\n            <span class=\"subtotal\">71.2<a-icon type=\"caret-up\" /></span>\r\n          </div>\r\n        </div>\r\n        <mini-area style=\"height: 45px\" />\r\n      </a-col>\r\n      <a-col style=\"padding: 0 34px; margin-bottom: 24px\" :sm=\"12\" :xs=\"24\">\r\n        <div class=\"num-info\">\r\n          <span class=\"title\">\r\n            {{$t('capita')}}\r\n            <a-tooltip :title=\"$t('introduce')\">\r\n              <a-icon type=\"info-circle\" style=\"font-size: 14px; margin-left: 8px\" />\r\n            </a-tooltip>\r\n          </span>\r\n          <div class=\"value\">\r\n            <span class=\"total\">2.7</span>\r\n            <span class=\"subtotal\">71.2<a-icon type=\"caret-down\" /></span>\r\n          </div>\r\n        </div>\r\n        <mini-area style=\"height: 45px\" />\r\n      </a-col>\r\n    </a-row>\r\n    <a-table\r\n      :dataSource=\"searchData\"\r\n      :columns=\"tableColumns\"\r\n      :pagination=\"{style: { marginBottom: 0 }, pageSize: 5}\"\r\n      size=\"small\"\r\n      rowKey=\"index\"\r\n    >\r\n      <a href=\"#/\" slot=\"keyword\" slot-scope=\"text\">{{text}}</a>\r\n      <span slot=\"rang\" slot-scope=\"text\">{{text}} %<a-icon type=\"caret-up\" /> </span>\r\n    </a-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MiniArea from '../../../components/chart/MiniArea'\r\n\r\nconst searchData = []\r\nfor (let i = 0; i < 50; i++) {\r\n  searchData.push({\r\n    index: i + 1,\r\n    keyword: '关键词-' + i,\r\n    count: Math.floor(Math.random() * 1000),\r\n    range: Math.floor(Math.random() * 100),\r\n    status: Math.floor((Math.random() * 10) % 2)\r\n  })\r\n}\r\n\r\nconst columns = [\r\n  {\r\n    dataIndex: 'index',\r\n    key: 'rank'\r\n  },\r\n  {\r\n    dataIndex: 'keyword',\r\n    key: 'keyword',\r\n    scopedSlots: {customRender: 'keyword'}\r\n  },\r\n  {\r\n    dataIndex: 'count',\r\n    key: 'users',\r\n    sorter: (a, b) => a.count - b.count\r\n  },\r\n  {\r\n    title: '周涨幅',\r\n    dataIndex: 'range',\r\n    key: 'range',\r\n    scopedSlots: {customRender: 'rang'}\r\n  }\r\n]\r\n\r\nexport default {\r\n  name: 'HotSearch',\r\n  components: {MiniArea},\r\n  i18n: require('./i18n-search'),\r\n  data () {\r\n    return {\r\n      searchData,\r\n      columns\r\n    }\r\n  },\r\n  computed: {\r\n    tableColumns() {\r\n      let columns = this.columns\r\n      return columns.map(item => {\r\n       item.title = this.$t(item.key)\r\n        return item\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .num-info{\r\n    .title{\r\n      color: @text-color-second;\r\n      font-size: 14px;\r\n      height: 22px;\r\n      line-height: 22px;\r\n      overflow: hidden;\r\n      text-overflow: ellipsis;\r\n      word-break: break-all;\r\n      white-space: nowrap;\r\n    }\r\n    .value{\r\n      .total{\r\n        color: @title-color;\r\n        display: inline-block;\r\n        line-height: 32px;\r\n        height: 32px;\r\n        font-size: 24px;\r\n        margin-right: 32px;\r\n      }\r\n      .subtotal{\r\n        color: @text-color-second;\r\n        font-size: 16px;\r\n        vertical-align: top;\r\n        margin-right: 0;\r\n        i{\r\n          font-size: 12px;\r\n          color: red;\r\n          transform: scale(.82);\r\n          margin-left: 4px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n</style>\r\n"]}]}