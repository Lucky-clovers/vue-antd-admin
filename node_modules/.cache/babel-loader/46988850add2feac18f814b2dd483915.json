":["<template>\r\n  <a-card :bordered=\"false\">\r\n    <a-steps class=\"steps\" :current=\"current\">\r\n      <a-step :title=\"$t('input')\" />\r\n      <a-step :title=\"$t('confirm')\" />\r\n      <a-step :title=\"$t('complete')\" />\r\n    </a-steps>\r\n    <div class=\"content\">\r\n      <step1 v-if=\"current === 0\" @nextStep=\"nextStep\"></step1>\r\n      <step2 v-if=\"current === 1\" @nextStep=\"nextStep\" @prevStep=\"prevStep\"></step2>\r\n      <step3 v-if=\"current === 2\" @prevStep=\"prevStep\" @finish=\"finish\"></step3>\r\n    </div>\r\n  </a-card>\r\n</template>\r\n\r\n<script>\r\nimport Step1 from './Step1'\r\nimport Step2 from './Step2'\r\nimport Step3 from './Step3'\r\n\r\nexport default {\r\n  name: 'StepForm',\r\n  i18n: require('./i18n'),\r\n  components: {Step1, Step2, Step3},\r\n  data () {\r\n    return {\r\n      current: 0\r\n    }\r\n  },\r\n  computed: {\r\n    desc() {\r\n      return this.$t('pageDesc')\r\n    }\r\n  },\r\n  methods: {\r\n    nextStep () {\r\n      if (this.current < 2) {\r\n        this.current += 1\r\n      }\r\n    },\r\n    prevStep () {\r\n      if (this.current > 0) {\r\n        this.current -= 1\r\n      }\r\n    },\r\n    finish () {\r\n      this.current = 0\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .steps{\r\n    max-width: 950px;\r\n    margin: 16px auto;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/pages/form/step"}]}